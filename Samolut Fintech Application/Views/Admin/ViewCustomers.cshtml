@{
}
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Middle Name</th>
            <th>Last Name</th>
            <th>Phone Number</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.CUSTOMER_ID</td>
                <td>@item.FIRST_NAME</td>
                <td>@item.MIDDLE_NAME</td>
                <td>@item.LAST_NAME</td>
                <td>@item.PHONE_NUMBER</td>
            </tr>
        }
    </tbody>
</table>